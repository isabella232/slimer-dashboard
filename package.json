{
  "name": "slimer-dashboard",
  "version": "0.0.0",
  "repository": "git@github.com:TryGhost/slimer-dashboard.git",
  "author": "Ghost Foundation",
  "license": "MIT",
  "main": "app.js",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src --ext .js --cache",
    "posttest": "yarn lint",
    "preship": "yarn lint",
    "ship": "STATUS=$(git status --porcelain); echo $STATUS; if [ -z \"$STATUS\" ]; then git push ${GHOST_UPSTREAM:-upstream} main --follow-tags; fi"
  },
  "files": [
    "public",
    "src"
  ],
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@primer/octicons-react": "^16.1.1",
    "@testing-library/jest-dom": "^5.0.0",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^14.0.0",
    "apollo3-cache-persist": "^0.13.0",
    "colord": "^2.9.1",
    "gitstar-components": "^1.0.5",
    "graphql": "^15.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "unfetch": "^4.1.0"
  },
  "devDependencies": {
    "eslint-plugin-ghost": "2.13.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:ghost/browser"
    ],
    "plugins": [
      "ghost"
    ],
    "rules": {
      "no-console": "warn"
    }
  },
  "eslintIgnore": [
    "src/serviceWorker.js"
  ],
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
